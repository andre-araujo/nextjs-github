{
    "env": {
      "development": {
        "presets": "next/babel",
        "plugins": [
            ["styled-components", { "ssr": true, "displayName": true, "preprocess": false } ]
        ]
      },
      "production": {
        "presets": "next/babel",
        "plugins": [
            ["styled-components", { "ssr": true, "displayName": true, "preprocess": true } ]
        ]
      },
      "test": {
        "presets": [
          ["env", { "modules": "commonjs" }],
          "next/babel"
        ]
      }
    }
  }
